					"server": "[parameters('SinkLinked_properties_typeProperties_server')]",
					"database": "[parameters('SinkLinked_properties_typeProperties_database')]",
					"encrypt": "mandatory",
					"trustServerCertificate": false,
					"authenticationType": "SQL",
					"userName": "[parameters('SinkLinked_properties_typeProperties_userName')]",
