{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "azure-df-one"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"SinkLinked_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SinkLinked'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://azurestorageaccountone.dfs.core.windows.net/"
		},
		"SinkLinked_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sql-db-server1.database.windows.net"
		},
		"SinkLinked_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "sql_db1"
		},
		"SinkLinked_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "admin123"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "azure-df-one_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"SinkLinked_password": {
						"value": "[parameters('SinkLinked_password')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"SinkLinked_properties_typeProperties_server": {
						"value": "[parameters('SinkLinked_properties_typeProperties_server')]"
					},
					"SinkLinked_properties_typeProperties_database": {
						"value": "[parameters('SinkLinked_properties_typeProperties_database')]"
					},
					"SinkLinked_properties_typeProperties_userName": {
						"value": "[parameters('SinkLinked_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}